#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules/@raycast/api/bin/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules/@raycast/api/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules/@raycast/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules/@raycast/api/bin/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules/@raycast/api/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules/@raycast/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/@raycast+api@1.87.4_@types+node@20.8.10_@types+react@18.2.27/node_modules:/Users/ewidlund/dev/polar-raycast/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run.js" "$@"
else
  exec node  "$basedir/../../bin/run.js" "$@"
fi
